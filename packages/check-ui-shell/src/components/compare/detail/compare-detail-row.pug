mixin contextgraphs
  +each('contextGraphRows as rowViewModel')
    .context-graph-row
      .spacer-flex
      ContextGraph(viewModel!='{rowViewModel.graphL}')
      .context-graph-spacer
      ContextGraph(viewModel!='{rowViewModel.graphR}')
      .spacer-flex
