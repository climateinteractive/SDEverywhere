mixin metarows
  +each('viewModel.metadataRows as row')
    .metadata-row
      .metadata-col
        .metadata-key { row.key }
        span &nbsp;
        .metadata-value { row.valueL || 'n/a' }
      .spacer-fixed
      .metadata-col
        .metadata-key { row.key }
        span &nbsp;
        .metadata-value { row.valueR || 'n/a' }

mixin datarows
  +each('viewModel.datasetRows as row')
    DatasetRow(viewModel!='{row}')
